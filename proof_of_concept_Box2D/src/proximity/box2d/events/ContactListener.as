package proximity.box2d.events{	import Box2D.Dynamics.Contacts.b2Contact;	import Box2D.Dynamics.b2ContactListener;	import Box2D.Dynamics.b2Fixture;	/**	 * @author Nicolas Zanotti	 */	public class ContactListener extends b2ContactListener	{		private var _main:Main;		private var _fixture:b2Fixture;		public function ContactListener(main:Main)		{			_main = main;		}		/**		 * Called when two fixtures begin to touch.		 */		override public function BeginContact(contact:b2Contact):void		{			_fixture = contact.GetFixtureA();			if ((_fixture.GetUserData() as Object).hasOwnProperty("name"))			{				if ((contact.GetFixtureA().GetUserData() as Object)["name"] as String == "magnet")				{					_main.onMagnetHit(contact.GetFixtureB().GetBody());					return;				}			}			_fixture = contact.GetFixtureB();			if ((_fixture.GetUserData() as Object).hasOwnProperty("name"))			{				if ((contact.GetFixtureB().GetUserData() as Object)["name"] as String == "magnet")				{					_main.onMagnetHit(contact.GetFixtureA().GetBody());				}			}		}	}}